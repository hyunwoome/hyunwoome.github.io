  <span class="k">return</span> <span class="nx">num</span> <span class="o">%</span> <span class="mi">2</span> <span class="o">===</span> <span class="mi">0</span> <span class="p">?</span> <span class="dl">'</span><span class="s1">Even</span><span class="dl">'</span> <span class="p">:</span> <span class="dl">'</span><span class="s1">Odd</span><span class="dl">'</span><span class="p">;</span>
